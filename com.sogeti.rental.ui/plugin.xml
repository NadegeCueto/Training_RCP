<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="com.sogeti.rental.ui.rentalcategory"
            class="com.sogeti.rental.ui.RentalView"
            id="com.sogeti.rental.ui.rentalview"
            name="RentalView"
            restorable="true">
      </view>
      <view
            category="com.sogeti.rental.ui.rentalcategory"
            class="com.sogeti.rental.ui.RentalTreeView"
            id="com.sogeti.rental.ui.rentaltreeview"
            name="RentalTreeView"
            restorable="true">
      </view>
      <category
            id="com.sogeti.rental.ui.rentalcategory"
            name="Rental">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.sogeti.rental.ui.RentalPerspective"
            icon="icons/Agency.png"
            id="com.sogeti.rental.ui.RentalPerspective"
            name="Rental">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.sogeti.rental.ui.RentalPreferencesInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.sogeti.rental.ui.RentalPreferencesPage"
            id="com.sogeti.rental.ui.rentalpreferencespage"
            name="RentalPreferencesPage">
      </page>
   </extension>

</plugin>
